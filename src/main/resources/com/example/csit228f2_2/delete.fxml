<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csit228f2_2.DeleteController">

    <VBox alignment="CENTER" layoutX="143.0" layoutY="44.0" prefHeight="246.0" prefWidth="337.0" spacing="10">
        <!-- Label for instructions -->
        <Label text="Enter Username to Delete:">
         <font>
            <Font size="19.0" />
         </font></Label>

        <!-- Text field for entering username -->
        <TextField fx:id="tfUsername" prefHeight="59.0" prefWidth="345.0" />

        <!-- Button to trigger delete action -->
        <Button onAction="#handleDelete" text="Delete">
         <font>
            <Font size="18.0" />
         </font></Button>
    </VBox>
   <Button fx:id="goBackID" layoutX="518.0" layoutY="14.0" mnemonicParsing="false" onAction="#onClickGoBack" text="GoBack" />
    <Label fx:id="messageLabel" />

</AnchorPane>
